{
	"apiVersion": 2,
	"name": "core/post-navigation-link",
	"title": "Post Navigation Link",
	"category": "design",
	"description": "Displays the next or previous post link that is adjacent to the current post.",
	"textdomain": "default",
	"attributes": {
		"textAlign": {
			"type": "string"
		},
		"type": {
			"type": "string",
			"default": "next"
		},
		"label": {
			"type": "string"
		},
		"showTitle": {
			"type": "boolean",
			"default": false
		}
	},
	"supports": {
		"reusable": false,
		"html": false
	}
}
