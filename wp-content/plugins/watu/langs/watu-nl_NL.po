msgstr "Regelmatig gestelde vragen"

#: views/help.html.php:13
msgid "Please check them <a href=\"%s\" target=\"_blank\">online</a>."
msgstr "Controleer het <a href=\"%s\" target=\"_blank\">online</a>."

