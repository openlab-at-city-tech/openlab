msgid "Frequently Asked Questions"
msgstr "Intrebări Frecvente"

#: views/help.html.php:13
msgid "Please check them <a href=\"%s\" target=\"_blank\">online</a>."
msgstr "Te rog verifică <b>%s</b> and <b>%s</b>online</a>."

